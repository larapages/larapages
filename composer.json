{
    "name": "nickdekruijk/larapages",
    "version": "0.1.0",
    "description": "A simple CMS build on Laravel 5",
    "license": "MIT",
	"keywords": [
		"cms",
		"laravel"
	],
	"homepage": "http://www.larapages.nl",
    "authors": [
        {
            "name": "Nick de Kruijk",
            "email": "git@nickdekruijk.nl"
        }
    ],
    "minimum-stability": "dev",
    "post-update-cmd": [
        "php artisan vendor:publish --tag=public --force"
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "^5.1",    
        "tinymce/tinymce": "^4.3"
    },
    "autoload": {
        "psr-0": {
            "NickDeKruijk\\LaraPages": "src/"
        }
    }
}
